                          <li>Category: <a href="#">Painting</a></li>
                        </ul>
                        <!-- <a href="#" class="more">Read More</a> -->
                      </div>
                    </div>
                    <!-- row -->
                  </div>
                </div>
                <!-- row -->
              </div>
